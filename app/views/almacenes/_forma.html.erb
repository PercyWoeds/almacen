<% form_for(@almacen) do |f| %>
  <%= f.error_messages %>

  <p>
    <%= f.label :nombre %>
    <%= f.text_field :nombre %>
  </p>
  <p>
    <%= f.label :direccion %>
    <%= f.text_field :direccion %>
  </p>
  <p>
    <%= f.label :codigo %>
    <%= f.text_field :codigo %>
  </p>
  <p>
    <%= f.submit 'Salvar' %>
  </p>
<% end %>
